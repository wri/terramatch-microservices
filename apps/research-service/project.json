{
  "name": "research-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/research-service/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build-repl": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "webpackConfig": "apps/research-service/repl.webpack.config.js",
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/research-service",
        "main": "apps/research-service/src/repl.ts"
      }
    },
    "repl": {
      "dependsOn": ["build-repl"],
      "command": "node dist/apps/research-service-repl"
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "research-service:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "research-service:build:development"
        },
        "production": {
          "buildTarget": "research-service:build:production"
        }
      }
    }
  }
}
